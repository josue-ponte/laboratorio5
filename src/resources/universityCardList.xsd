<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.example.org/Product"
           xmlns:tns="http://www.example.org/Product">

    <xs:element name="universityCardList">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="universityCard" type="tns:universityCard" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="universityCard">
        <xs:sequence>
            <xs:element name="university"		type="xs:string"/>
            <xs:element name="code"			    type="xs:string"/>
            <xs:element name="dni"				type="xs:string"/>
            <xs:element name="lastName"		    type="xs:string"/>
            <xs:element name="firstName"		type="xs:string"/>
            <xs:element name="faculty"			type="xs:string"/>
            <xs:element name="career"			type="xs:string"/>
            <xs:element name="year"				type="xs:integer"/>
            <xs:element name="incomeYear"		type="xs:string"/>
            <xs:element name="expirationDate"	type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>